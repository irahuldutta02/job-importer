PORT=4000
MONGO_URI=mongodb://localhost:27017/job_importer
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
CONCURRENCY=4
BATCH_SIZE=50

# Job Feed URLs (comma-separated)
FEED_URLS=https://jobicy.com/?feed=job_feed,https://jobicy.com/?feed=job_feed&job_categories=smm&job_types=full-time,https://jobicy.com/?feed=job_feed&job_categories=data-science,https://www.higheredjobs.com/rss/articleFeed.cfm

# Cron schedule (hourly by default - set to */5 * * * * for every 5 minutes in development)
IMPORT_CRON_SCHEDULE=0 * * * *

# Logging level
LOG_LEVEL=info

# Development settings
NODE_ENV=development
